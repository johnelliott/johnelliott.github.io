{
  "name": "blog",
  "version": "1.0.0",
  "description": "blog",
  "main": "build.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "budo --live",
    "sass-watch": "node_modules/node-sass/bin/node-sass --watch style.scss style.css",
    "sasquatch": "npm run sass-watch",
