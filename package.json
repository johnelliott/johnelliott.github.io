{
  "name": "blog",
  "version": "1.0.0",
  "description": "blog",
  "main": "build.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "NODE_ENV=development node_modules/.bin/node-sass --source-map . style.scss style.css",
    "sass-watch": "npm run sass && NODE_ENV=development node_modules/.bin/node-sass --watch --source-map . style.scss style.css",
    "dev": "npm run build-dev && node_modules/.bin/budo --live --open",
    "build-dev": "npm run sass && NODE_ENV=development node build.js",
    "build": "node build.js"
  },
  "keywords": [
    "blog"
  ],
  "author": "John Elliott <johnelliott703@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "clean-css": "^3.4.19",
    "marked": "^0.3.5",
    "node-sass": "^3.8.0"
  },
  "dependencies": {
    "budo": "^9.2.1"
  }
}
